using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace LoopStatement_do_while_
{
    class Program
    {
        static void Main(string[] args)
        {
            int sum = 0;
            int i = 0;
            do
            {
                sum = sum + i;
                i++;
            } while (i <= 100);
            Console.WriteLine("From 1 to 100,sum is "+sum);
            Console.ReadLine();
        }
    }
}
