using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConditionalStatement3
{
    class Program
    {
        static void Main(string[] args)
        {
            int score = 56;
            string grade = "";
            if(score > 90)
            {
                grade = "excellent";
            }
            else if(score >= 80)
            {
                grade = "great";
            }
            else if(score >= 60)
            {
                grade = "pass";
            }
            else
            {
                grade = "fail";
            }
            Console.WriteLine("This student's grade is "+grade);
            Console.ReadLine();

        }
    }
}
