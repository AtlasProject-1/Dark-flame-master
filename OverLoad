using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace OverLoad
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 10;
            int b = 5;
            int c = 1;
            Console.WriteLine(Add(a,b));
            Console.WriteLine();
            Console.WriteLine(Add(a,b,c));
            Console.ReadLine();
        }
        static int Add(int x,int y)
        {
            return x + y;
        }
        static int Add(int x,int y,int z)
        {
            return x + y + z;
        }

    }
}
