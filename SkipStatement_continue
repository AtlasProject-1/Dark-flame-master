using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SkipStatement_continue
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = 1;
            for(i=1;i<=30;i++)
            {
                if(i%2 != 0)
                {
                    Console.WriteLine(i);
                }
                else
                {
                    continue;
                }
            }
            Console.ReadLine();

        }
    }
}
